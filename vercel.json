{
  "version": 2,
  "builds": [
    {
      "src": "web_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/web_app.py"
    }
  ],
  "env": {
    "FLASK_APP": "web_app.py",
    "FLASK_ENV": "production"
  },
  "functions": {
    "web_app.py": {
      "maxDuration": 30
    }
  }
}